{
	"name": "svelte-headless-table",
	"description": "Unopinionated and extensible data tables for Svelte",
	"version": "0.17.7",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .prettierignore --check --plugin-search-dir=. . && eslint --ignore-path .eslintignore .",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"test": "jest --collect-coverage",
		"test:watch": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/bryanmylee/svelte-headless-table.git"
	},
	"keywords": [
		"svelte",
		"table",
		"datagrid",
		"datatable",
		"headless",
		"plugin",
		"sorting",
		"filtering",
		"ordering",
		"hiding",
		"grouping",
		"selecting",
		"expanding",
		"resizing"
	],
	"author": "Bryan Lee",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bryanmylee/svelte-headless-table/issues"
	},
	"homepage": "https://svelte-headless-table.bryanmylee.com",
	"devDependencies": {
		"@babel/core": "7.23.6",
		"@babel/preset-env": "^7.23.6",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.2.3",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/svelte": "^4.0.5",
		"@types/faker": "5",
		"@types/jest": "^29.5.11",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"faker": "5",
		"jest": "^29.7.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte-check": "^3.6.2",
		"svelte-jester": "^3.0.0",
		"svelte-preprocess": "^5.1.2",
		"svelte2tsx": "^0.6.27",
		"ts-jest": "^29.1.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.9"
	},
	"dependencies": {
		"svelte-keyed": "^1.1.7",
		"svelte-render": "^1.6.1",
		"svelte-subscribe": "^1.0.6"
	},
	"peerDependencies": {
		"svelte": "^4.2.8"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*"
	],
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./plugins": {
			"types": "./dist/lib/plugins/index.d.ts",
			"svelte": "./dist/plugins/index.js",
			"import": "./dist/plugins/index.js"
		}
	},
	"type": "module"
}
